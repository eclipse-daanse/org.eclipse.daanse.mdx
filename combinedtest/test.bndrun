-tester: biz.aQute.tester.junit-platform



-runtrace: true
-runvm: ${def;argLine}

-resolve.effective: active

-runee: JavaSE-21
-runfw: org.eclipse.osgi
# This will help us keep -runbundles sorted
-runstartlevel: \
	order=sortbynameversion,\
	begin=-1


-runrequires: \
	bnd.identity;id='org.eclipse.daanse.mdx.combinedtest',\
	bnd.identity;id='org.eclipse.daanse.mdx.combinedtest-tests',\
	bnd.identity;id=junit-jupiter-engine,\
